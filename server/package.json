{
  "name": "server",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "app.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "fastify start -l info dist/app.js",
    "build": "tsc -p tsconfig.json",
    "t": "tsc --traceResolution",
    "watch:ts": "tsc -w",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch:ts\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts$ -w -l info -P dist/app.js"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "5.7.1",
    "@fastify/cors": "8.3.0",
    "@fastify/mongodb": "8.0.0",
    "@fastify/sensible": "5.2.0",
    "@fastify/type-provider-typebox": "3.5.0",
    "@sinclair/typebox": "0.31.14",
    "fastify": "4.23.1",
    "fastify-cli": "5.8.0",
    "fastify-plugin": "4.5.1",
    "firebase-admin": "11.10.1",
    "gravatar": "1.8.2",
    "module-alias": "2.2.3",
    "mongoose": "7.5.1"
  },
  "devDependencies": {
    "@types/gravatar": "1.8.3",
    "@types/node": "20.6.0",
    "@types/tap": "15.0.9",
    "concurrently": "8.2.1",
    "fastify-tsconfig": "1.0.1",
    "tap": "16.3.8",
    "ts-node": "10.9.1",
    "typescript": "5.2.2"
  },
  "_moduleAliases": {
    "@": "dist"
  }
}
